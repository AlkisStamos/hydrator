{
  "name": "alkis-stamos/hydrator",
  "description": "Hydrate or extract data from PHP classes to arrays and back",
  "license": "MIT",
  "keywords": ["hydrator","extract","map"],
  "authors": [{
	"name": "Alkis Stamos",
	"email": "stamosalkis@gmail.com"
  }],
  "require": {
	"php": "^7.1",
	"alkis-stamos/metadata": "dev-master",
    "cache/filesystem-adapter": "^1.0"
  },
  "repositories": [{
	"type": "vcs",
	"url": "git@github.com:AlkisStamos/metadata.git",
	"no-api": true
  }],
  "autoload": {
	"psr-4": {
	  "AlkisStamos\\Hydrator\\": "src"
	},
	"classmap": ["mock"]
  },
  "autoload-dev": {
	"psr-4": {
	  "AlkisStamos\\Hydrator\\Tests\\": "tests"
	}
  },
  "require-dev": {
	"phpunit/phpunit": "^7.4"
  }
}
